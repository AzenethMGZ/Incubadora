<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.incubadora">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logoofi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Incubadora"
        tools:targetApi="31">
        <activity
            android:name=".index"
            android:exported="false"
            android:label="@string/title_activity_index"
            android:theme="@style/Theme.Incubadora.NoActionBar" />
        <activity
            android:name=".agregarvisi"
            android:exported="false" />
        <activity
            android:name=".agregarincu"
            android:exported="false" />
        <activity
            android:name=".visi"
            android:exported="false" />
        <activity
            android:name=".sensores"
            android:exported="false" />
        <activity
            android:name=".temperatura"
            android:exported="false" />
        <activity
            android:name=".Registro"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".Login"
            android:exported="true"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>